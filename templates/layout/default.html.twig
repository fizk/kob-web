    {% block meta %}{% endblock %}
    <link rel="search" type="application/opensearchdescription+xml" href="{{ path(language|default('is') == 'is' ? 'leit' : 'search') }}?q=%s" title="Kling&amp;Bang">
    <link rel="shortcut icon" href="/favicon.ico">
    <link rel="icon" type="image/png" href="/favicon/favicon-32x32.png" sizes="32x32" />
