    <link rel="stylesheet" href="/styles/bundle.css" />
    <link rel="alternate" type="application/rss+xml" title="Subscribe to Shows/News/Projects feed" href="{{ host }}{{ path('rss') }}" />
    {% block stylesheets %}{% endblock %}
</head>
<body class="app">
    {% block content %}{% endblock %}

    <footer class="app__footer">
        <ul class="app__nav-list-language">
        {% block language_switch %}
            {% if language|default('is') == 'is' %}
                <li class="app__nav-list-item"><a class="app__nav-link" href="{{ path('home') }}">English</a></li>
            {% else %}
                <li class="app__nav-list-item"><a class="app__nav-link" href="{{ path('heim') }}">Íslenska</a></li>
            {% endif %}
        {% endblock %}
        </ul>
        <nav class="app__nav">
            <ul class="app__nav-list app__nav-list--quarter">
                <li class="app__nav-brand-item">
                    {% include '@partials/header.html.twig' with {color: 'white'} %}
                </li>
                {% if language|default('is') == 'is' %}
                    <li style="margin-top: 0.5rem" class="app__nav-list-item"><a href="{{ path('verslun') }}" class="app__nav-link">Verk til sölu</a></li>
                    <li class="app__nav-list-item"><a href="{{ path('velunnarar') }}" class="app__nav-link">Vinir Kling &amp; Bang</a></li>
                {% else %}

                    <li style="margin-top: 0.5rem" class="app__nav-list-item"><a href="{{ path('store') }}" class="app__nav-link">Works for sale</a></li>
                    <li class="app__nav-list-item"><a href="{{ path('supporters') }}" class="app__nav-link">Friends of Kling &amp; Bang</a></li>
                {% endif %}
            </ul>
            <ul class="app__nav-list app__nav-list--quarter">
                {% if language|default('is') == 'is' %}
                    <li style="margin-top: 0" class="app__nav-menu-item"><a href="{{ path('listamenn') }}" class="app__nav-link">Listamenn</a></li>
                    <li class="app__nav-menu-item"><a href="{{ path('verkefni') }}" class="app__nav-link">Verkefni</a></li>
                    <li class="app__nav-menu-item"><a href="{{ path('um') }}" class="app__nav-link">Um Kling &amp; Bang</a></li>
                {% else %}
                    <li style="margin-top: 0" class="app__nav-menu-item"><a href="{{ path('authors') }}" class="app__nav-link">Artists</a></li>
                    <li class="app__nav-menu-item"><a href="{{ path('projects') }}" class="app__nav-link">Projects</a></li>
                    <li class="app__nav-menu-item"><a href="{{ path('about') }}" class="app__nav-link">About Kling &amp; Bang</a></li>
                {% endif %}
            </ul>
            <ul class="app__nav-list app__nav-list--half app__nav-list-years">
            {% for item in entry_years %}
                <li class="app__nav-year-list">
                    {% if year|default(0) == item.year %}
                    <span class="app__nav-years">{{ item.year }}</span>
                    {% else %}
                    <a href="{{ path(language|default('is') == 'is' ? 'listi' : 'list', {year: item.year}) }}" class="app__nav-years--active">{{ item.year }}</a>
                    {% endif %}
                </li>
            {% endfor %}
            </ul>

        </nav>

        <div class="app__nav">

            <ul class="app__nav-list app__nav-list--quarter">
                {% if language|default('is') == 'is' %}
                    <li class="app__opening-hours-item">Mið - Sun: 12 – 18</li>
                    <li class="app__opening-hours-item">Fim: 12 – 21</li>
                    <li class="app__opening-hours-item">Mán/Þri: Lokað</li>
